{
  "name": "experiments",
  "version": "1.0.0",
  "author": "@asus4",
  "scripts": {
    "start": "npm-run-all --parallel *:dev",
    "build": "npm-run-all *:dist",
    "js:dev": "webpack --watch",
    "js:dist": "webpack -p",
    "pug:dev": "pug src/index.pug -P -w -o public",
    "pug:dist": "pug src/index.pug -P -o public",
    "css:dev": "stylus --use ./node_modules/poststylus --with \"['autoprefixer']\" -w src/main.styl -o public/ -u nib --sourcemap-inline",
    "css:dist": "stylus --use ./node_modules/poststylus --with \"['autoprefixer']\" src/main.styl -c -o public/ -u nib"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.7",
    "dat-gui": "^0.5.0",
    "exports-loader": "^0.6.4",
    "glsl-lut": "^1.1.0",
    "glslify": "^6.1.0",
    "glslify-loader": "^1.0.2",
    "json-loader": "^0.5.7",
    "nib": "^1.1.2",
    "npm-run-all": "^4.1.2",
    "poststylus": "^1.0.0",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6",
    "raw-loader": "^0.5.1",
    "simplex-noise": "^2.3.0",
    "stats.js": "^0.17.0",
    "stylus": "^0.54.5",
    "three": "^0.89.0",
    "webpack": "^3.10.0"
  }
}
