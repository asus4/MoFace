// Stop flex warning while using postcss
// https://github.com/tj/nib/issues/312
flex-version = flex
support-for-ie = false
vendor-prefixes = official

@import 'nib'

global-reset()



#main
  position: fixed  
  width: 100%
  height: 100%  
  canvas
    position: absolute
    width: 100%
    height: 100%
