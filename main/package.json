{
  "name": "invisi-morph",
  "version": "1.0.0",
  "author": "@asus4",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all --parallel *:dev",
    "build": "cross-env NODE_ENV=production npm-run-all *:dist",
    "js:dev": "webpack --watch",
    "js:dist": "webpack -p",
    "pug:dev": "pug src/index.pug -P -w -o public",
    "pug:dist": "pug src/index.pug -P -o public",
    "css:dev": "stylus --use ./node_modules/poststylus --with \"['autoprefixer']\" -w src/main.styl -o public/ -u nib --sourcemap-inline",
    "css:dist": "stylus --use ./node_modules/poststylus --with \"['autoprefixer']\" src/main.styl -c -o public/ -u nib",
    "svgo": "svgo public/images/*/*.svg"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "cardinal-spline-js": "^2.3.6",
    "clmtrackr": "^1.1.2",
    "compression-webpack-plugin": "^1.1.3",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "dat-gui": "^0.5.0",
    "delaunay-fast": "^1.0.1",
    "exports-loader": "^0.6.4",
    "glsl-lut": "^1.1.0",
    "glslify": "^6.1.0",
    "glslify-loader": "^1.0.2",
    "json-loader": "^0.5.7",
    "mathjs": "^3.18.0",
    "mobile-detect": "^1.4.1",
    "nib": "^1.1.2",
    "npm-run-all": "^4.1.2",
    "poststylus": "^1.0.0",
    "preload-js": "^0.6.3",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6",
    "querystring": "^0.2.0",
    "raw-loader": "^0.5.1",
    "shuffle-array": "^1.0.1",
    "stats.js": "^0.17.0",
    "stylus": "^0.54.5",
    "svgo": "^1.0.3",
    "three": "^0.89.0",
    "tone": "^0.11.12",
    "vue": "^2.5.13",
    "vue-loader": "^13.6.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0"
  }
}
